{
  "name": "af-sergiu/api-invoker",
  "description": "PHP library for api calls",
  "type": "library",
  "authors": [
    {
      "name": "af-sergiu",
      "email": "af.sergiu@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "AfSergiu\\ApiInvoker\\": "src/",
      "AfSergiu\\ApiInvoker\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": "^7.8",
    "psr/container": "^2.0"
